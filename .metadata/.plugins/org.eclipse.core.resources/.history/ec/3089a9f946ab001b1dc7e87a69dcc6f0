#include <stdio.h>
#include <stdlib.h>
#include "funcRec.h"

char funcRec(int);

int main(void) {
	puts("!!!Hello World in c!!!"); /* prints !!!Hello World!!! */
	funcRec(1000);
	return EXIT_SUCCESS;
}

char funcRec(int cnt)
{
	char tab[64];
	char ret = 1;

	for (char i = 0 ; i <=63 ; i++)
	{
		tab[i] = 1;
	}
	if (cnt>0)
	{
		funcRec(cnt--);
	}
	else
	{
		return ret;
	}

	return ret;
}
